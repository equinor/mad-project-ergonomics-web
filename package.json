  "scripts": {
    "start-message": "babel-node tools/startMessage.js",
    "prestart": "npm-run-all --parallel start-message",
    "start": "npm-run-all --parallel test:watch open:src lint:watch",
    "open:src": "babel-node tools/srcServer.js",
    "open:dist": "babel-node tools/distServer.js",
    "lint": "esw webpack.config.* src tools --color",
    "lint:watch": "npm run lint -- --watch",
    "test": "jest",
    "test:watch": "npm run test -- --watch",
    "clean-build": "npm run remove-build && mkdir build && mkdir build/dist",
    "remove-build": "rimraf ./build",
    "create-config": "babel-node tools/generateConfig.js",
    "prebuild": "npm run clean-build",
    "build": "babel-node tools/build.js",
    "analyse": "babel-node tools/analyseBundle.js",
    "eslint-check": "eslint --print-config path/to/main.js | eslint-config-prettier-check",
    "lint:css": "stylelint './src/**/*.js'"
  },
